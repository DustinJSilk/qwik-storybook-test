{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adaptor'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "storybook": "NODE_PATH=./node_modules NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6005",
    "storybook.build": "NODE_PATH=./node_modules NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "storybook.deploy": "chromatic --project-token=043a80cb9d5b --build-script-name=storybook.build -o=storybook-static"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.15.2",
    "@builder.io/qwik-city": "0.0.128",
    "@types/eslint": "8.4.10",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "eslint": "8.28.0",
    "eslint-plugin-qwik": "0.15.2",
    "node-fetch": "3.3.0",
    "prettier": "2.8.0",
    "typescript": "4.9.3",
    "vite": "3.2.4",
    "vite-tsconfig-paths": "3.5.0",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-backgrounds": "^6.5.14",
    "@storybook/addon-docs": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-measure": "^6.5.14",
    "@storybook/addon-outline": "^6.5.14",
    "@storybook/addons": "^6.5.14",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/channel-postmessage": "^6.5.14",
    "@storybook/channel-websocket": "^6.5.14",
    "@storybook/client-api": "^6.5.14",
    "@storybook/core-common": "^6.5.14",
    "@storybook/core-server": "^6.5.14",
    "@storybook/html": "^6.5.14",
    "@storybook/preview-web": "^6.5.14",
    "@storybook/test-runner": "^0.9.1",
    "@storybook/theming": "^6.5.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
